<!DOCTYPE html>
<html>
<head>
  <title>Gradebook</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<header class="navigation" role="banner">
  <div class="navigation-wrapper">
    <a href="javascript:void(0)" class="logo">
      <img src="https://raw.githubusercontent.com/thoughtbot/refills/master/source/images/placeholder_logo_1.png" alt="Logo Image">
    </a>
    <a href="javascript:void(0)" class="navigation-menu-button" id="js-mobile-menu">MENU</a>
    <nav role="navigation">
      <ul id="js-navigation-menu" class="navigation-menu show">
        <li class="nav-link"> <%= link_to "New Assignment", new_assignment_path, class: "navbar" %> </li>
        <li class="nav-link">   <%= link_to "All Assignments", assignments_path, class: "navbar" %> </li>
        <li class="nav-link"> <%= link_to "New Student", new_student_path, class: "navbar" %> </li>
        <li class="nav-link">   <%= link_to "View Students", students_path, class: "navbar" %> </li>
        <li class="nav-link">   <%= link_to "View Parents", parents_path, class: "navbar" %> </li>
        <a><%= link_to "Logout", sessions_logout_path if session[:user_id] %></a>

      </ul>

    </nav>

  </div>
</header>
